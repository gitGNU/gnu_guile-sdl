evtype "event-type"
#:infile "SDL_events.h"
#:region ("SDL_NOEVENT = 0"
          "SDL_USEREVENT")
#:symrgx (ws ("SDL_[A-Z]+") ",")
#:kxform (4 downcase
            ("event$" "")
            ("^(sys|video|mouse|joy)" 1 "-")
            ("(motion|up|down)$" "-" 1)
            ("--" "-"))

